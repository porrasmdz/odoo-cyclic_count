<odoo>
  
    <!-- actions opening views on models -->

    <record id="cyclic_product_registry_tree" model="ir.ui.view">
      <field name="name">cyclic.product.registy.tree</field>
      <field name="model">cyclic.product.registry</field>
      <field name="arch" type="xml">
        <tree string="Lista de Productos (Registros)" multi_edit="1">
          <field name="product_id" string="Producto" />
          <field name="system_units" string="U. Sistema" />
          <field name="real_units" string="U. Reales" />
          <field name="difference" string="Diferencia" />
          <field name="status" string="Estado" />
        </tree>
      </field>
    </record>
    
  
  


    <record id="product_registry_action" model="ir.actions.act_window" >
      <field name="name">Productos (Registros)</field>
      <field name="res_model">cyclic.product.registry</field>
      <field name="view_mode">tree,form</field>
    </record>


   
    
</odoo>
