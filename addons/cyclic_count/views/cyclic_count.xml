<odoo>
  
     <record model="ir.ui.view" id="cyclic_count.list">
      <field name="name">cyclic_count list</field>
      <field name="model">cyclic.count</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="company"/>
          <field name="start_date"/>
          <field name="end_date"/>
          <field name="asignee_ids"/>
          <field name="status"/>
          <field name="warehouse_id"/>
          <field name="product_ids"/>
          
        </tree>
      </field>
    </record>
    
    <record model="ir.ui.view" id="cyclic_count.form">
      <field name="name">cyclic_count form</field>
      <field name="model">cyclic.count</field>
      <field name="arch" type="xml">
        <form>
          
          
          <header>
            <button action="" class="oe_highlight" type="object" string="Finalizar Conteo"/>
            <field name="status" widget="statusbar"/>
          </header>
          <h1 class="oe_title">
            <label for="name"/>
            <field name="name"/>
          </h1>
          <group >
            <group >
              <field name="start_date" />
              
              <field name="asignee_ids" widget="many2many_tags"/>
              <field name="company" />
              
            </group>
            
            <group>
              <field name="end_date"/>
              
              <field name="status"/>
              
              <field name="warehouse_id" />
            </group>
          </group>
          <notebook>
            <page string="Conteo">
              
              <group >
                <field name="product_ids" string=""/>
          
              </group>
            </page>
          </notebook>
        </form>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record  id="cyclic_count_action" model="ir.actions.act_window">
      <field name="name">Conteos CÃ­clicos</field>
      <field name="res_model">cyclic.count</field>
      <field name="view_mode">tree,form</field>
    </record>


   
    
</odoo>
